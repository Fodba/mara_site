{% block title %}Medium{% endblock %}

{% block gestion %}
    <h1>Medium</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ medium.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ medium.nom }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ medium.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ medium.email }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ medium.photo }}</td>
            </tr>
            <tr>
                <th>Photo_texte</th>
                <td>{{ medium.photoTexte }}</td>
            </tr>
            <tr>
                <th>Biographie</th>
                <td>{{ medium.biographie }}</td>
            </tr>
            <tr>
                <th>Pratique</th>
                <td>{{ medium.pratique }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_gestion_medium_index') }}">back to list</a>

    <a href="{{ path('app_gestion_medium_edit', {'id': medium.id}) }}">edit</a>

    {{ include('medium/_delete_form.html.twig') }}
